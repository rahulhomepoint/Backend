    "key_highlights": {
        "id": "key_highlights_form",
        "title": "Enter Project Key Highlights",
        "fields": [
            {
                "label": "Highlights",
                "name": "highlights",
                "type": "repeater",
                "min_items": 1,
                "fields": [
                    {
                        "label": "Title",
                        "name": "title",
                        "type": "text",
                        "placeholder": "e.g. Prime Location",
                        "required": true
                    },
                    {
                        "label": "Description",
                        "name": "description",
                        "type": "textarea",
                        "placeholder": "Enter the highlight details",
                        "required": true
                    },
                    {
                        "label": "Image",
                        "name": "image",
                        "type": "file",
                        "accept": [
                            "image/png",
                            "image/jpeg",
                            "image/webp"
                        ],
                        "required": false
                    }
                ]
            }
        ]
    },
    "location_advantage": {
        "id": "location_advantage_form",
        "title": "Location Advantage Setup",
        "fields": [
            {
                "label": "Section Title",
                "name": "section_title",
                "type": "text",
                "default": "Emami Aastha - Location Advantage"
            },
            {
                "label": "Section Subtitle",
                "name": "section_subtitle",
                "type": "text",
                "default": "The perfect balance of convenience, and great connectivity,"
            },
            {
                "label": "Location Map Embed URL",
                "name": "map_embed_url",
                "type": "url",
                "required": true,
                "placeholder": "Enter Google Maps embed iframe link"
            },
            {
                "label": "Location Categories",
                "name": "location_categories",
                "type": "repeater",
                "min_items": 1,
                "fields": [
                    {
                        "label": "Category Name",
                        "name": "category",
                        "type": "text",
                        "placeholder": "e.g., Necessities"
                    },
                    {
                        "label": "Items",
                        "name": "items",
                        "type": "array",
                        "item_type": "text",
                        "placeholder": "e.g., Hospitals, Schools, Parks"
                    }
                ]
            }
        ]
    },
    "layout_and_floorplan": {
        "id": "layout_and_floorplan_form",
        "title": "Layout & Floor Plans Input",
        "fields": [
            {
                "label": "Section Title",
                "name": "section_title",
                "type": "text",
                "default": "Emami Aastha Layout & Floor plans"
            },
            {
                "label": "Section Subtitle",
                "name": "section_subtitle",
                "type": "text",
                "default": "Discover the Blueprint of Luxury"
            },
            {
                "label": "Tabs",
                "name": "tabs",
                "type": "array",
                "item_type": "text",
                "default": [
                    "Master Plan",
                    "2 BHK",
                    "3 BHK",
                    "4 BHK"
                ]
            },
            {
                "label": "Master Layout Description",
                "name": "master_layout_description",
                "type": "textarea",
                "default": "At Emami Aastha, luxury living is complemented by modern conveniences..."
            },
            {
                "label": "Carpet Area (sq.ft.)",
                "name": "carpet_area",
                "type": "text",
                "default": "1000-1400"
            },
            {
                "label": "Saleable Area (sq.ft.)",
                "name": "saleable_area",
                "type": "text",
                "default": "1500-2100"
            },
            {
                "label": "Number of Car Parks",
                "name": "car_parks",
                "type": "number",
                "default": 1
            },
            {
                "label": "Master Plan Image",
                "name": "master_plan_image",
                "type": "file",
                "accept": [
                    "image/png",
                    "image/jpeg",
                    "image/webp"
                ]
            },
            {
                "label": "Downloadable PDF Link",
                "name": "download_pdf_url",
                "type": "url",
                "placeholder": "https://yourdomain.com/floorplan.pdf"
            }
        ]
    },
    "about": {
        "id": "about_emami_section_form",
        "title": "About Emami Section Setup",
        "fields": [
            {
                "label": "Left Section - Image or Logo",
                "name": "left_image",
                "type": "file",
                "accept": [
                    "image/png",
                    "image/jpeg",
                    "image/webp"
                ],
                "required": false
            },
            {
                "label": "Left Section Title",
                "name": "left_title",
                "type": "text",
                "default": "Read our Story"
            },
            {
                "label": "Right Section: About Emami Realty - Title",
                "name": "realty_title",
                "type": "text",
                "default": "About Emami Realty"
            },
            {
                "label": "About Emami Realty - Description",
                "name": "realty_description",
                "type": "textarea",
                "default": "Emami Realty, the real estate arm of Emami Group was incorporated in 2006..."
            },
            {
                "label": "Right Section: About Emami Group - Title",
                "name": "group_title",
                "type": "text",
                "default": "About Emami Group"
            },
            {
                "label": "About Emami Group - Description",
                "name": "group_description",
                "type": "textarea",
                "default": "Emami is a growth story of a dream that started with a humble beginning..."
            },
            {
                "label": "Call-to-Action Button",
                "name": "cta_button",
                "type": "group",
                "fields": [
                    {
                        "label": "Button Text",
                        "name": "text",
                        "type": "text",
                        "default": "Read More"
                    },
                    {
                        "label": "Button Link",
                        "name": "link",
                        "type": "url",
                        "placeholder": "https://yourdomain.com/about"
                    }
                ]
            }
        ]
    }
}